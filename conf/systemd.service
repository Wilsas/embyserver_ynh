[Unit]
Description=Emby Server is a personal media server with apps on just about every device.
After=network.target

[Service]
Type=simple
User=__APP__
Group=__APP__
ExecStart=/usr/bin/emby-server start
Restart=always
TimeoutSec=20
ExecStopPost=/usr/bin/emby-server clear

[Install]
WantedBy=multi-user.target
